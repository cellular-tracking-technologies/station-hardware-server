    "modem-status-driver": "file:../dependencies/modem-status-driver",
    "usb-storage-driver": "file:../dependencies/usb-storage-driver",
    "wifi-config": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
